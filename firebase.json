{
  "firestore": {
    "database": "prosportpages1",
    "location": "southamerica-east1",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules",
    "bucket": "prosport-portfolio.firebasestorage.app"
  },
  "functions": {
    "source": "functions",
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ],
    "codebase": "default"
  },
  "apphosting": [
    {
      "backends": [
        {
          "backendId": "prosport",
          "rootDir": "/",
          "ignore": [
            "node_modules",
            ".git",
            "firebase-debug.log",
            "firebase-debug.*.log",
            "functions"
          ]
        },
        {
          "backendId": "studio",
          "rootDir": "/",
          "ignore": [
            "node_modules",
            ".git",
            "firebase-debug.log",
            "firebase-debug.*.log",
            "functions"
          ],
          "alwaysDeployFromSource": true
        }
      ]
    },
    {
      "backendId": "studio",
      "rootDir": "/",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions"
      ],
      "alwaysDeployFromSource": true
    }
  ],
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
